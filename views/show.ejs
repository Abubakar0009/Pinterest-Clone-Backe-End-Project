<% include ./partials/header.ejs %>

<div class="  w-full min-h-screen bg-zinc-800 py-8 text-white px-10  ">
    <div class="card flex flex-wrap gap-10  ">
        <%user.posts.forEach(function(elm){%>
            <div class="w-32">
                <div class=" card w-full min-h-40 rounded-lg bg-zinc-300 overflow-hidden  ">
                    <img class="h-full w-full object-cover "
                        src="/images/uploads/<%= elm.image%>"alt="wrong">
                        
                </div>
               <h6 class="block leading-none text-lg mt-2 "><%=elm.title%></h6>
    
            </div>
            
            <% }) %>

    </div>


</div>
<% include ./partials/footer.ejs %>

